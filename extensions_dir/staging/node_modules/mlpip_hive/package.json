{
  "name": "mlpip_hive",
  "version": "0.2.1",
  "description": "A JupyterLab extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/my_name/myextension",
  "bugs": {
    "url": "https://github.com/my_name/myextension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "ashwinkumar",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{scss,css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/my_name/myextension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.3.4",
    "@jupyterlab/apputils": "^3.3.4",
    "@jupyterlab/docregistry": "^2.2.8",
    "@jupyterlab/mainmenu": "^3.0.5",
    "@jupyterlab/notebook": "^3.3.4",
    "@jupyterlab/services": "^6.3.4",
    "@lumino/disposable": "^1.4.3",
    "@phosphor/widgets": "^1.9.3",
    "@types/lodash": "^4.14.168",
    "@types/react-select": "^3.0.11",
    "jupyterlab_toastify": "^4.1.3",
    "react-select": "^3.0.8"
  },
  "devDependencies": {
    "rimraf": "^2.7.1",
    "typescript": "^3.7.7"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
